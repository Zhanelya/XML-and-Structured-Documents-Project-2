      <xs:element maxOccurs="unbounded" ref="u"/>
      <xs:element maxOccurs="unbounded" ref="pause"/>
      <xs:element maxOccurs="unbounded" ref="vocal"/>
      <xs:element maxOccurs="unbounded" ref="event"/>
      <xs:element maxOccurs="unbounded" ref="shift"/>
      <xs:element maxOccurs="unbounded" ref="trunc"/>
