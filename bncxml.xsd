      <xs:element minOccurs="0" ref="pause"/>
      <xs:element minOccurs="0" ref="vocal"/>
      <xs:element minOccurs="0" ref="event"/>
      <xs:element minOccurs="0" ref="shift"/>
      <xs:element minOccurs="0" ref="trunc"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="model.divPart.spoken_sequenceOptionalRepeatable">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="u"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="pause"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="vocal"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="event"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="shift"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="trunc"/>
