  <xs:element name="gi">
    <xs:annotation>
      <xs:documentation>(generic identifier) contains the name (generic identifier) of an element.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="gi.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="gi.attributes">
    <xs:attributeGroup ref="att.identifiable.attribute.ident"/>
    <xs:attributeGroup ref="att.identifiable.attribute.ns"/>
  </xs:attributeGroup>
  <xs:element name="ident">
    <xs:annotation>
      <xs:documentation>contains an identifier or name for an object of some kind in a formal language</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="ident.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="ident.attributes">
    <xs:attributeGroup ref="att.identifiable.attribute.ident"/>
    <xs:attributeGroup ref="att.identifiable.attribute.ns"/>
  </xs:attributeGroup>
  <xs:element name="attList" type="attList.content">
    <xs:annotation>
      <xs:documentation>contains documentation for all the attributes associated with this
